{
  "edges": [
    {
      "from": "input-3",
      "to": "promptTemplate-1",
      "out": "text",
      "in": "topic"
    },
    {
      "from": "input-3",
      "to": "urlTemplate-2",
      "out": "text",
      "in": "query"
    },
    {
      "from": "urlTemplate-2",
      "to": "fetch-4",
      "out": "url",
      "in": "url"
    },
    {
      "from": "jsonata-5",
      "to": "promptTemplate-1",
      "out": "result",
      "in": "headlines"
    },
    {
      "from": "xmlToJson-6",
      "to": "jsonata-5",
      "out": "json",
      "in": "json"
    },
    {
      "from": "fetch-4",
      "to": "xmlToJson-6",
      "out": "response",
      "in": "xml"
    },
    {
      "from": "generateText-7",
      "to": "output-8",
      "out": "completion",
      "in": "text"
    },
    {
      "from": "secrets-9",
      "to": "generateText-7",
      "out": "PALM_KEY",
      "in": "PALM_KEY"
    },
    {
      "from": "promptTemplate-1",
      "to": "generateText-7",
      "out": "prompt",
      "in": "text"
    }
  ],
  "nodes": [
    {
      "id": "promptTemplate-1",
      "type": "promptTemplate",
      "configuration": {
        "template": "Use the news headlines below to write one or two sentences to summarize the latest news on this topic:\n\n##Topic:\n{{topic}}\n\n## Headlines {{headlines}}\n\\n## Summary:\n"
      }
    },
    {
      "id": "urlTemplate-2",
      "type": "urlTemplate",
      "configuration": {
        "template": "https://news.google.com/rss/search?q={{query}}&hl=en-US&gl=US&ceid=US:en"
      }
    },
    {
      "id": "input-3",
      "type": "input",
      "configuration": {
        "message": "What news topic would you like to have summarized?"
      }
    },
    {
      "id": "fetch-4",
      "type": "fetch"
    },
    {
      "id": "jsonata-5",
      "type": "jsonata",
      "configuration": {
        "expression": "$join((rss.channel.item.title.`$t`)[[1..20]], `\n`)"
      }
    },
    {
      "id": "xmlToJson-6",
      "type": "xmlToJson"
    },
    {
      "id": "generateText-7",
      "type": "generateText"
    },
    {
      "id": "output-8",
      "type": "output"
    },
    {
      "id": "secrets-9",
      "type": "secrets",
      "configuration": {
        "keys": ["PALM_KEY"]
      }
    }
  ],
  "kits": [
    {
      "url": "npm:@google-labs/llm-starter"
    }
  ]
}
