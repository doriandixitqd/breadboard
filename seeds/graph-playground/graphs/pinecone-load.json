{
  "title": "Loading Chunks into Pinecone",
  "description": "This board is a simple example of loading chunked data into Pinecone.",
  "version": "0.0.1",
  "edges": [
    {
      "from": "map-1",
      "to": "output-2",
      "out": "list",
      "in": "text"
    },
    {
      "from": "get-content",
      "to": "map-1",
      "out": "result",
      "in": "list"
    },
    {
      "from": "load-chunks",
      "to": "get-content",
      "out": "response",
      "in": "json"
    },
    {
      "from": "url",
      "to": "load-chunks",
      "out": "text",
      "in": "url"
    }
  ],
  "nodes": [
    {
      "id": "url",
      "type": "input"
    },
    {
      "id": "load-chunks",
      "type": "fetch"
    },
    {
      "id": "get-content",
      "type": "jsonata",
      "configuration": {
        "expression": "content.$zip($keys(),*)[[0..3]].{\"id\": $[0],\"text\": text,\"metadata\": info}"
      }
    },
    {
      "id": "map-1",
      "type": "map",
      "configuration": {
        "board": {
          "kind": "board",
          "board": {
            "edges": [
              {
                "from": "embedString-4",
                "to": "merge",
                "out": "embedding",
                "in": "embedding"
              },
              {
                "from": "secrets-5",
                "to": "embedString-4",
                "out": "PALM_KEY",
                "in": "PALM_KEY"
              },
              {
                "from": "jsonata-3",
                "to": "embedString-4",
                "out": "result",
                "in": "text"
              },
              {
                "from": "input-1",
                "to": "jsonata-3",
                "out": "item",
                "in": "json"
              },
              {
                "from": "merge",
                "to": "output-2",
                "out": "accumulator",
                "in": "item"
              },
              {
                "from": "input-1",
                "to": "merge",
                "out": "item",
                "in": "accumulator"
              }
            ],
            "nodes": [
              {
                "id": "input-1",
                "type": "input"
              },
              {
                "id": "output-2",
                "type": "output"
              },
              {
                "id": "merge",
                "type": "append"
              },
              {
                "id": "jsonata-3",
                "type": "jsonata",
                "configuration": {
                  "expression": "text"
                }
              },
              {
                "id": "embedString-4",
                "type": "embedString"
              },
              {
                "id": "secrets-5",
                "type": "secrets",
                "configuration": {
                  "keys": [
                    "PALM_KEY"
                  ]
                }
              }
            ],
            "kits": [
              {
                "url": "npm:@google-labs/node-nursery"
              },
              {
                "url": "npm:@google-labs/llm-starter"
              }
            ]
          }
        }
      }
    },
    {
      "id": "output-2",
      "type": "output"
    }
  ],
  "kits": [
    {
      "url": "npm:@google-labs/llm-starter"
    },
    {
      "url": "npm:@google-labs/node-nursery"
    }
  ]
}