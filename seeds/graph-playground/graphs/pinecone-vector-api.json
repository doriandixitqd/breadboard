{
  "title": "Pinecone API vector call builder",
  "description": "This board helps making [vector API calls](https://docs.pinecone.io/reference/describe_index_stats_post) to Pinecone.",
  "version": "0.0.1",
  "edges": [
    {
      "from": "secrets-1",
      "to": "make-headers",
      "out": "PINECONE_API_KEY",
      "in": "json"
    },
    {
      "from": "make-headers",
      "to": "pinecone-api-call",
      "out": "result",
      "in": "headers"
    },
    {
      "from": "secrets-2",
      "to": "make-pinecone-url",
      "out": "PINECONE_INDEX",
      "in": "PINECONE_INDEX"
    },
    {
      "from": "secrets-3",
      "to": "make-pinecone-url",
      "out": "PINECONE_PROJECT_ID",
      "in": "PINECONE_PROJECT_ID"
    },
    {
      "from": "secrets-4",
      "to": "make-pinecone-url",
      "out": "PINECONE_ENVIRONMENT",
      "in": "PINECONE_ENVIRONMENT"
    },
    {
      "from": "api",
      "to": "make-pinecone-url",
      "out": "call",
      "in": "call"
    },
    {
      "from": "make-pinecone-url",
      "to": "pinecone-api-call",
      "out": "prompt",
      "in": "url"
    },
    {
      "from": "api",
      "to": "pinecone-api-call",
      "out": "body",
      "in": "body"
    },
    {
      "from": "pinecone-api-call",
      "to": "response",
      "out": "response",
      "in": "response"
    }
  ],
  "nodes": [
    {
      "id": "api",
      "type": "input",
      "configuration": {
        "schema": {
          "type": "object",
          "properties": {
            "api": {
              "type": "string",
              "title": "API call",
              "description": "The API call to make"
            },
            "body": {
              "type": "object",
              "title": "Body",
              "description": "The body of the API call"
            }
          },
          "required": [
            "api"
          ]
        }
      }
    },
    {
      "id": "make-headers",
      "type": "jsonata",
      "configuration": {
        "expression": "{ \"Api-Key\": $, \"Accept\": \"application/json\", \"Content-Type\": \"application/json\" }"
      }
    },
    {
      "id": "secrets-1",
      "type": "secrets",
      "configuration": {
        "keys": [
          "PINECONE_API_KEY"
        ]
      }
    },
    {
      "id": "pinecone-api-call",
      "type": "fetch",
      "configuration": {
        "raw": false,
        "method": "POST"
      }
    },
    {
      "id": "make-pinecone-url",
      "type": "promptTemplate",
      "configuration": {
        "template": "https://{{PINECONE_INDEX}}-{{PINECONE_PROJECT_ID}}.svc.{{PINECONE_ENVIRONMENT}}.pinecone.io/{{call}}"
      }
    },
    {
      "id": "secrets-2",
      "type": "secrets",
      "configuration": {
        "keys": [
          "PINECONE_INDEX"
        ]
      }
    },
    {
      "id": "secrets-3",
      "type": "secrets",
      "configuration": {
        "keys": [
          "PINECONE_PROJECT_ID"
        ]
      }
    },
    {
      "id": "secrets-4",
      "type": "secrets",
      "configuration": {
        "keys": [
          "PINECONE_ENVIRONMENT"
        ]
      }
    },
    {
      "id": "response",
      "type": "output"
    }
  ],
  "kits": [
    {
      "url": "npm:@google-labs/llm-starter"
    }
  ]
}