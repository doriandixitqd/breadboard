      "id": "get-tools-object",
      "type": "react-helper",
      "configuration": { "method": "getToolsObject" }
    },
    {
      "id": "get-tools",
      "type": "jsonata",
      "configuration": { "expression": "$keys() ~> $join(', ')" }
    },
    {
      "id": "get-tool-descriptions",
      "type": "jsonata",
      "configuration": {
        "expression": "$zip($keys(), $.*) ~> $map(function($v, $k) { $join($v, ': ') }) ~> $join('\n')"
      }
    },
